<script>
  const squares = document.querySelectorAll(".color-square");
  console.log("Total squares found:", squares.length);

  const colors = [
    "bg-green1",
    "bg-green2",
    "bg-green3",
    "bg-green4",
    "bg-green5",
  ];

  // Animate fade-in on page load
  squares.forEach((square, index) => {
    setTimeout(() => {
      square.classList.add("fade-in");
      console.log(`Fade-in added to square #${index}`);
    }, index * 10);
  });

  function randomColor() {
    const color = colors[Math.floor(Math.random() * colors.length)];
    console.log("Random color chosen:", color);
    return color;
  }

  function changeRandomSquare() {
    const squareIndex = Math.floor(Math.random() * squares.length);
    const square = squares[squareIndex];
    const oldColor = [...square.classList].find((c) =>
      c.startsWith("bg-green"),
    );

    console.log(`Changing square #${squareIndex}`, "Old color:", oldColor);

    if (oldColor) {
      square.classList.remove(oldColor);
      console.log(`Removed old color: ${oldColor}`);
    }

    const newColor = randomColor();
    square.classList.add(newColor);
    console.log(`Added new color: ${newColor}`);
  }

  setTimeout(() => {
    console.log("Starting color-changing interval...");
    setInterval(changeRandomSquare, 150);
  }, squares.length * 50);
</script>
