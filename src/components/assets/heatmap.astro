<script>
  const squares = document.querySelectorAll(".color-square");
  const colors = [
    "bg-green1",
    "bg-green2",
    "bg-green3",
    "bg-green4",
    "bg-green5",
  ];

  function randomColor() {
    return colors[Math.floor(Math.random() * colors.length)];
  }

  function changeRandomSquare() {
    const square = squares[Math.floor(Math.random() * squares.length)];
    const oldColor = [...square.classList].find((c) =>
      c.startsWith("bg-green")
    );
    if (oldColor) square.classList.remove(oldColor);
    square.classList.add(randomColor());
  }

  setInterval(changeRandomSquare, 150);
</script>

<style>
  .color-square {
    transition: background-color 0.5s;
  }
</style>
